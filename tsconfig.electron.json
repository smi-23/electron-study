{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "module": "CommonJS",
    "outDir": "dist",
    "rootDir": "electron",
    "moduleResolution": "node",
    "resolveJsonModule": true,
  },
  "include": ["electron/electron.ts", "electron/preload.ts"]
}
